test -n "$USERDIR" || exit 1
OLDMOS=$USERDIR/tmp.OLDMOS

# Part 1: generate MOs
cat <<. >ZMAT
Generate OLDMOS
H
F 1 R

R=1.4

*ACES2
BASIS=DZP,SPHERICAL=ON
REF=UHF
CALC=SCF,HFSTABILITY=FOLLOW

.
ln -s $GENBAS . || exit 1
xaces2 || exit 1
mv NEWMOS $OLDMOS || exit 1

# clean
for file in *
do case $file in
   GENBAS|ZMAT.BAS) ;;
   *) rm -rf $file ;;
   esac
done

# Part 2: optimize the geometry
cat <<. >ZMAT
Use OLDMOS (without a GUESS file)
H
F 1 R*

R=1.4

*ACES2
!RESTART
BASIS=DZP,SPHERICAL=ON
REF=UHF,GUESS=READ_SO_MOS
CALC=CCSD,DROPMO=1

.
mv $OLDMOS OLDMOS || exit 1
xaces2 || exit 1

exit 0
