#include "ecpints.h"
#include <string>
#include <map>

void load_ecps(map<string,map<string,vector<ecpdat> > > &ecp_lib, map<string,map<string,int> > &ecp_core, string libname)
{
   // read from ECP library in ACESII format
   // resolve path
   string fname = "./"+libname;
   char * pDYNROOT = getenv("DYNROOT");
   if (pDYNROOT!=NULL) fname=string(pDYNROOT)+"/lib/"+libname;

   // check for existence and open
   struct stat fInfo;
   if (stat(fname.c_str(),&fInfo)!=0) printf("Failed to locate ECP library %s\n",fname.c_str());
   ifstream lib(fname.c_str());

   // iterate over the file
   string line;
   vector<string> tok;
   while (getline(lib,line))
   {
      // remove comment from line
      line=line.substr(0,line.find_first_of('!'));

      // check for beginning * (continue otherwise)
      int fchar = line.find_first_not_of(' ');
      if ((fchar==string::npos)||(line[fchar]=='*')) continue;

      // we are beginning a section
      // get atom and basis in lowercase
      transform(line.begin(),line.end(),line.begin(),static_cast<int(*)(int)>(tolower));
      tok.clear();
      Tokenize(line,tok," :");
      string atom = tok[0],
             bset = tok[1];

      // reject two lines
      if (!getline(lib,line)) break;
      if (!getline(lib,line)) break;

      // get number of core electrons
      if (!getline(lib,line)) break;
      tok.clear();
      Tokenize(line,tok," ");
      int ncore = atoi(tok[2].c_str());
      ecp_core[bset][atom]=ncore;

      // read in the actual functions
      int l = -2;
      while (getline(lib,line))
      {
         // check for end of section
         if (line.find_first_of('*')!=string::npos) break;

         // tokenize line
         tok.clear();
         Tokenize(line,tok," ");

         // if number of tokens=1, it is a new potential
         if (tok.size()==1)
         {
           l++;
         } else {
            // otherwise, it is an actual function
            double coef = atof(tok[0].c_str()),
                   exp  = atof(tok[2].c_str());
            int N = atoi(tok[1].c_str());
            ecp_lib[bset][atom].push_back((ecpdat){N,l,coef,exp,-1});
         }
      };
   };

   // close the file
   lib.close();

